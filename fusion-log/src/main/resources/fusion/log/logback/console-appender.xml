<included>
    <appender name="console_out" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>${fusion.log.logback.console-pattern:-%d [%thread] %-5level %logger{36} %line - %msg%n}</pattern>
        </encoder>
    </appender>
</included>
